module Boolean where

data Boolean : Type where
  T
  F

not : Boolean -> Boolean
ind not x = case x of
  T -> F
  F -> T

id : Boolean -> Boolean
id = \x . x

notnot : Boolean -> Boolean
notnot = \x. not (not x)

test : Eq[not (not F), F] : Boolean
test = refl tt

test2 : Eq[id, notnot] : (Boolean -> Boolean)
ind test2 x = case x of
  T -> refl tt
  F -> refl tt
